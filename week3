score = int(input("Enter your score (0-100): "))
if score >= 90:
    grade = "A"
elif score >= 80:
    grade = "B"
elif score >= 70:
    grade = "C"
elif score >= 60:
    grade = "D"
else:
    grade = "F"
print(f"Your grade is: {grade}")
if grade == "A":
    print("Excellent work!")
elif grade =="B" :
    print("You did well ! ")
elif grade =="C":
    print("Satisfying Work")
elif grade =="D":
    print("you can do better")
else :
    print("be careful !")
